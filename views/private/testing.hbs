<div class="container mt-5">
    <div class="row row-cols-1 row-cols-md-3 g-4">
  
        <div class="col">
        <div class="card card-container-lists">
            <div class="card-body card-header-lists">
            <h5 class="card-title items-title"><a href="">Name</a>

            </h5>
            </div>
            <div class="card-body">
            <p><strong>Number of Spaces:</strong>{{spaces.length}}</p>
            <p><strong>Number of members: </strong>{{members.length}}</p>

            {{!-- EACH --}}
            <div class="d-flex">
              <img src="/images/logo1.png" class="logo-items " alt="">
              <p class="align-self-center profile-name">Oswaldo</p>
            </div>

            <div class="row row-cols-12 row-cols-lg-2 g-4 botones-contenedor-lista">
                <div class="col mx-auto ">
                <a class="see-next-btn" href="/profile/lists/{{id}}">See all folders</a>
                </div>
                <div class="col mx-auto">
                <a class="delete-button" href="/profile/folders/delete/{{id}}">Delete</a>
                </div>
            </div>
            </div>
        </div>
        </div>


  </div>

</div>



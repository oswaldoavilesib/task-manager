<h1 class="workspace-titles">Spaces</strong></h1>

<div class="table-responsive table-container">
    <table class="table folders-table table-hover">
    <thead>
        <tr>
        <th scope="col">User</th>
        <th scope="col">Team's Name</th>
        <th scope="col">Number of members</th>
        <th scope="col">List of members</th>
        <th scope="col">Go to your lists</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                <li class="table-uri"><img class="profile-image-spaces"src="/images/defaultprofile.png" alt=""></li>
                <span>Oswaldo</span>
            </td>
            <td>My team's name</td>
            <td>#</td>
            <td class="table-uri"> 
                <li>Fer</li>
                <li>John</li>
            </td>

            <td><a href="/profile/lists/{{id}}">See all lists.</a></td>
        </tr>
    </table>
</div>





{{#each spaces}}
<div>
    <img src="{{avatar}}" class="card-img-top" alt="...">
    <h5 class="card-title"><strong>Team's name: {{name}}</strong></h5>
    <h5 class="card-title"><strong>Members</strong></h5>
    <p>Number of members: {{members.length}}</p>
    <p>List of members</p>
    <a href="/profile/folders/{{id}}">See all folders</a>
    {{#each members}}
        <ul>
            <li><span><img src="{{user.profilePicture}}" alt=""></span>{{user.username}}</li>
        </ul>
    {{/each}}
</div>
{{/each}}
